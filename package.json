{
  "name": "shijbey.github.io",
  "version": "1.0.0",
  "description": "Personal Portfolio Site",
  "dependencies": {
    "@types/node": "^10.12.18",
    "@types/seedrandom": "^2.4.27",
    "@types/three": "^0.93.13",
    "seedrandom": "^2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-minify": "^0.5.0",
    "browserify": "^16.2.3",
    "source-map-loader": "^0.2.1",
    "three": "^0.100.0",
    "typescript": "^3.2.2",
    "webpack": "^2.3.3"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:generator_types && npm run build:generator_js",
    "build:generator_types": "tsc",
    "build:generator_js": "babel ./src --out-dir ./dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:web": "browserify ./dist/SwordGenerator.js -o ./dist/bundle.js",
    "minify": "babel ./dist/bundle.js --out-dir ./dist --minify",
    "bundle": "webpack"
  },
  "repository": {
    "type": "git",
